{
  "author": "addyosmani <addyosmani@gmail.com>",
  "description": "Faster subsequent page-loads by prefetching in-viewport links during idle time",
  "filename": "quicklink.js",
  "keywords": [
    "prefetch",
    "performance",
    "fetch",
    "intersectionobserver",
    "background",
    "speed"
  ],
  "license": "Apache-2.0",
  "name": "quicklink",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoogleChromeLabs/quicklink.git"
  },
  "version": "2.0.0",
  "autoupdate": {
    "source": "npm",
    "target": "quicklink",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*.js"
        ]
      }
    ]
  }
}
